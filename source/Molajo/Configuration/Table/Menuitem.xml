<?xml version="1.0" encoding="utf-8"?>
<model
        name="Menuitem"
        table="#__content"
        primary_key="id"
        data_source="JDatabase">
    <filters include="ContentFilters"/>
    <fields>
        <field name="custom_fields" registry="MenuitemCustomfields">
            <element name="custom_field1" filter="char" null="1" default="" values=""/>
        </field>
        <field name="parameters" registry="MenuitemParameters">
            <element name="caching" filter="binary" null="1" default="1" values="0,1"/>
            <element name="cache_time" filter="integer" null="1" default="15" values=""/>
            <element name="cache_handler" filter="char" null="1" default="" values=""/>
            <element name="list_limit" filter="integer" null="1" default="15" values=""/>
            <element name="theme_id" filter="integer" null="1" default="" values=""/>
            <element name="page_view_id" filter="integer" null="1" default="" values=""/>
            <element name="template_view_id" filter="integer" null="1" default="" values=""/>
            <element name="wrap_view_id" filter="integer" null="1" default="" values=""/>
        </field>
        <field name="metadata" registry="MenuitemMetadata">
            <element name="metadata_title" filter="char" null="0" default=""/>
            <element name="metadata_description" filter="text" null="0" default=""/>
            <element name="metadata_keywords" filter="char" null="1" default=""/>
            <element name="metadata_robots" filter="char" null="0" default="follow,index"
                     values="follow,nofollow,index,noindex"/>
            <element name="metadata_author" filter="char" null="1" default=""/>
            <element name="metadata_content_rights" filter="char" null="1" default=""/>
        </field>
        <foreignkeys include="ContentForeignkeys"/>
        <triggers include="ContentTriggers"/>
    </fields>
</model>