<?xml version="1.0" encoding="utf-8"?>
<model
        name="SiteApplications"
        table="#__site_applications"
        primary_key="id"
        db="JDatabase">
    <filters>
        <filter name="site_id" filter="int" null="0" default=""/>
        <filter name="application_id" filter="int" null="0" default=""/>
    </filters>
    <fks>
        <fk name="site_id" source_id="id" source_model="Sites" required="1"/>
        <fk name="application_id" source_id="id" source_model="Applications" required="1"/>
    </fks>
</model>

