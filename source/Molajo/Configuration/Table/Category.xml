<?xml version="1.0" encoding="utf-8"?>
<model
        name="Category"
        table="#__content"
        primary_key="id"
        primary_prefix="a"
        get_customfields="0"
        get_item_children="0"
        use_special_joins="0"
        check_view_level_access="0"
        data_source="JDatabase">
    <table>
        <item>
            <fields>
                <include name="ContentFields"/>
            </fields>
            <foreignkeys>
                <foreignkey include="ContentForeignkeys"/>
            </foreignkeys>
            <children>
                <child include="ContentChildren"/>
            </children>
            <triggers>
                <trigger include="ContentTriggers"/>
            </triggers>
            <customfields>
                <customfield name="customfields">
                    <include name="ContentCustomfields"/>
                </customfield>
                <customfield name="metadata">
                    <include name="SystemMetadata"/>
                </customfield>
                <customfield name="parameters">
                    <include name="SystemParameters"/>
                </customfield>
            </customfields>
        </item>
    </table>
</model>
