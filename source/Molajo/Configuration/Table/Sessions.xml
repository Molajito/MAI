<?xml version="1.0" encoding="utf-8"?>
<model
        name="Sessions"
        table="#__group_view_groups"
        primary_key="id"
        data_source="JDatabase">
    <filters>
        <filter name="session_id" filter="char" null="0" default=""/>
        <filter name="application_id" filter="int" null="0" default=""/>
        <filter name="session_time" filter="char" null="0" default=""/>
        <filter name="data" filter="" null="1" default=""/>
        <filter name="user_id" filter="int" null="0" default=""/>
    </filters>
    <foreignkeys>
        <foreignkey name="user_id" source_id="id" source_model="UsersModel" required="1"/>
        <foreignkey name="application_id" source_id="id" source_model="ApplicationsModel" required="1"/>
    </foreignkeys>
</model>
