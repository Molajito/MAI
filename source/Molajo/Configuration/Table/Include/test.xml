<?xml version="1.0" encoding="utf-8"?>
<extension type="component" version="1.0">
    <name>COMPONENT_ARTICLES_NAME</name>
    <author>Amy Stephen</author>
    <create_date>June 2012</create_date>
    <copyright>Copyright (C) 2012 Amy Stephen. All rights reserved.</copyright>
    <license>GNU General Public License version 2 or later; see LICENSE.txt</license>
    <author_email>collaborate@molajo.org</author_email>
    <author_url>http://molajo.org</author_url>
    <version>1.0</version>
    <description>COMPONENT_ARTICLES_DESCRIPTION</description>
    <config>
        <model
                name="Articles"
                catalog_type_id="10000"
                table="#__content"
                primary_key="id"
                primary_prefix="a"
                data_source="JDatabase">
        </model>
        <table>
            <item>
                <fields>
                    <field name="id" field="int" null="1" default="" identity="1"/>
                    <field name="extension_instance_id" field="int" null="0" default="0"/>
                    <field name="catalog_type_id" field="int" null="0" default="0"/>
                    <field name="title" field="char" null="0" default=" "/>
                    <field name="subtitle" field="char" null="1" default=" "/>
                    <field name="path" field="char" null="1" default=" "/>
                    <field name="alias" field="char" null="1" default=" "/>
                    <field name="content_text" field="text" null="1" default=""/>
                    <field name="protected" field="boolean" null="0" default="0"/>
                    <field name="featured" field="boolean" null="0" default="0"/>
                    <field name="stickied" field="boolean" null="0" default="0"/>
                    <field name="status" field="integer" null="0" default="0"/>
                    <field name="start_publishing_datetime" field="date" null="0" default="0000-00-00 00:00:00"/>
                    <field name="stop_publishing_datetime" field="date" null="0" default="0000-00-00 00:00:00"/>
                    <field name="version" field="int" null="0" default="1"/>
                    <field name="version_of_id" field="int" null="0" default="0"/>
                    <field name="status_prior_to_version" field="int" null="0" default="0"/>
                    <field name="created_datetime" field="date" null="0" default="0000-00-00 00:00:00"/>
                    <field name="created_by" field="int" null="0" default="0"/>
                    <field name="modified_datetime" field="date" null="0" default="0000-00-00 00:00:00"/>
                    <field name="modified_by" field="int" null="0" default="0"/>
                    <field name="checked_out_datetime" field="date" null="0" default="0000-00-00 00:00:00"/>
                    <field name="checked_out_by" field="int" null="0" default="0"/>
                    <field name="root" field="int" null="0" default="0"/>
                    <field name="parent_id" field="int" null="0" default="0"/>
                    <field name="lft" field="int" null="0" default="0"/>
                    <field name="rgt" field="int" null="0" default="0"/>
                    <field name="lvl" field="int" null="0" default="0"/>
                    <field name="home" field="int" null="0" default="0"/>
                    <field name="position" field="char" length="45" null="0" default="0"/>
                    <field name="custom_fields" customfield="1"/>
                    <field name="parameters" customfield="1"/>
                    <field name="metadata" customfield="1"/>
                    <field name="language" field="char" null="1" default=""/>
                    <field name="translation_of_id" field="int" null="0" default="0"/>
                    <field name="ordering" field="int" null="0" default="0"/>
                </fields>

                <foreignkeys>
                    <foreignkey name="catalog_type_id" source_id="id" source_model="CatalogTypes" required="1"/>
                    <foreignkey name="checked_out_by" source_id="id" source_model="Users" required="1"/>
                    <foreignkey name="created_by" source_id="id" source_model="Users" required="1"/>
                    <foreignkey name="modified_by" source_id="id" source_model="Users" required="1"/>
                    <foreignkey name="extension_instance_id" source_id="id" source_model="ExtensionInstances"
                                required="1"/>
                    <foreignkey name="catalog_type_id" source_id="id" source_model="CatalogTypes" required="1"/>
                </foreignkeys>
                <children>
                    <child name="Comments" join="id:parent_id"/>
                    <child name="Ratings" join="id:parent_id"/>
                    <child name="Notifications" join="id:parent_id"/>
                </children>

                <triggers>
                    <trigger name="Alias"/>
                    <trigger name="CatalogTypeId"/>
                    <trigger name="ContentText"/>
                    <trigger name="DateFields"/>
                    <trigger name="UserFields"/>
                    <trigger name="Status"/>
                    <trigger name="Customfields"/>
                    <trigger name="Metadata"/>
                    <trigger name="Parameters"/>
                    <trigger name="ViewAccess"/>
                </triggers>
                <customfields>
                    <customfield name="custom_fields" registry="ItemCustomfields">
                        <element name="intro_image" file="" filter="image" shape="" size=""/>
                        <element name="body_image" file="" filter="image" shape="" size=""/>
                        <element name="author_notes" field="char" null="0" default=" "/>

                        <element name="only_in_custom" file="" filter="image" shape="" size=""/>
                    </customfield>
                    <customfield name="metadata" registry="ItemMetadata">
                        <element name="metadata_title" filter="char" null="1" default=""/>
                        <element name="metadata_description" filter="text" null="1" default=""/>
                        <element name="metadata_keywords" filter="char" null="1" default=""/>
                        <element name="metadata_robots" filter="char" null="1" default="follow,index"/>
                        <element name="metadata_author" filter="char" null="1" default=""/>
                        <element name="metadata_content_rights" filter="char" null="1" default=""/>

                    </customfield>
                    <customfield name="parameters" registry="ItemParameters">
                        <element name="enable_draft_save" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_version_history" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="maximum_version_count" filter="integer" null="0" default="5" minimum="0"
                                 maximum="20"/>
                        <element name="enable_hit_counts" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_comments" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_ratings" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_notifications" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_tweets" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_ping" filter="binary" null="0" default="1" values="0,1"/>

                        <element name="display_view_on_no_results" filter="binary" null="0" default="0" values="0,1"/>

                        <element name="theme_id" filter="integer" null="1" default="" values=""/>

                        <element name="page_view_id" filter="integer" null="1" default="" values=""/>
                        <element name="page_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="page_view_css_class" filter="char" null="1" default="" values=""/>

                        <element name="item_template_view_id" filter="integer" null="1" default="" values=""/>
                        <element name="item_template_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="item_template_view_css_class" filter="char" null="1" default="" values=""/>
                        <element name="item_wrap_view_id" filter="integer" null="1" default="0" values=""/>
                        <element name="item_wrap_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="item_wrap_view_css_class" filter="char" null="1" default="" values=""/>
                        <element name="item_get_special_fields" filter="integer" null="0" default="1" values="0,1,2"/>
                        <element name="item_get_children" filter="integer" null="0" default="1" values="0,1,2"/>
                        <element name="item_check_view_level_access" filter="integer" null="0" default="1"
                                 values="0,1,2"/>

                        <element name="form_template_view_id" filter="integer" null="1" default="" values=""/>
                        <element name="form_template_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="form_template_view_css_class" filter="char" null="1" default="" values=""/>
                        <element name="form_wrap_view_id" filter="integer" null="1" default="0" values=""/>
                        <element name="form_wrap_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="form_wrap_view_css_class" filter="char" null="1" default="" values=""/>

                        <element name="caching" filter="binary" null="1" default="1" values="0,1"/>
                        <element name="cache_time" filter="integer" null="1" default="15" values=""/>
                        <element name="cache_handler" filter="char" null="1" default="" values=""/>

                    </customfield>
                </customfields>
            </item>
            <component>
                <customfields>
                    <customfield name="parameters" registry="ComponentParameters">
                        <element name="enable_draft_save" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_version_history" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="maximum_version_count" filter="integer" null="0" default="5" minimum="0"
                                 maximum="20"/>
                        <element name="enable_hit_counts" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_comments" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_ratings" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_notifications" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_tweets" filter="binary" null="0" default="1" values="0,1"/>
                        <element name="enable_ping" filter="binary" null="0" default="1" values="0,1"/>

                        <element name="display_view_on_no_results" filter="binary" null="0" default="0" values="0,1"/>

                        <element name="theme_id" filter="integer" null="1" default="" values=""/>

                        <element name="page_view_id" filter="integer" null="1" default="" values=""/>
                        <element name="page_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="page_view_css_class" filter="char" null="1" default="" values=""/>

                        <element name="item_template_view_id" filter="integer" null="1" default="" values=""/>
                        <element name="item_template_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="item_template_view_css_class" filter="char" null="1" default="" values=""/>
                        <element name="item_wrap_view_id" filter="integer" null="1" default="0" values=""/>
                        <element name="item_wrap_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="item_wrap_view_css_class" filter="char" null="1" default="" values=""/>
                        <element name="item_get_special_fields" filter="integer" null="0" default="1" values="0,1,2"/>
                        <element name="item_get_children" filter="integer" null="0" default="1" values="0,1,2"/>
                        <element name="item_check_view_level_access" filter="integer" null="0" default="1"
                                 values="0,1,2"/>

                        <element name="form_template_view_id" filter="integer" null="1" default="" values=""/>
                        <element name="form_template_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="form_template_view_css_class" filter="char" null="1" default="" values=""/>
                        <element name="form_wrap_view_id" filter="integer" null="1" default="0" values=""/>
                        <element name="form_wrap_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="form_wrap_view_css_class" filter="char" null="1" default="" values=""/>

                        <element name="list_page_view_id" filter="integer" null="1" default=""/>
                        <element name="list_page_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="list_page_view_css_class" filter="char" null="1" default="" values=""/>
                        <element name="list_template_view_id" filter="integer" null="1" default=""/>
                        <element name="list_template_view_css_id" filter="char" null="1" default="" values=""/>
                        <element name="list_template_view_css_class" filter="char" null="1" default="" values=""/>

                        <element name="list_get_special_fields" filter="integer" null="0" default="1" values="0,1,2"/>
                        <element name="list_get_child_row_counts" filter="integer" null="0" default="1" values="0,1,2"/>
                        <element name="list_check_view_level_access" filter="integer" null="0" default="1"
                                 values="0,1,2"/>

                        <element name="list_display_archived_content" filter="integer" null="0" default="1"
                                 values="0,1,2"/>
                        <element name="list_display_featured_content" filter="integer" null="0" default="1"
                                 values="0,1,2"/>
                        <element name="list_display_stickied_content" filter="integer" null="0" default="1"
                                 values="0,1,2"/>
                        <element name="list_display_published_date_begin" filter="date" null="0" default=""/>
                        <element name="list_display_published_date_end" filter="date" null="0" default=""/>
                        <element name="list_display_category_list"/>
                        <element name="list_display_tag_list"/>
                        <element name="list_display_author_list"/>
                        <element name="list_begin" filter="integer" default="1"/>
                        <element name="list_length" filter="integer" default="5" minimum="0" maximum="20"/>
                        <element name="list_order_by_field1" filter="field_list" null="1" default="featured"/>
                        <element name="list_order_by_direction1" filter="char" null="1" default="DESC"
                                 values="ASC,DESC"/>
                        <element name="list_order_by_field2" filter="field_list" null="1"
                                 default="start_publishing_datetime"/>
                        <element name="list_order_by_direction2" filter="char" null="1" default="DESC"
                                 values="ASC,DESC"/>
                        <element name="list_order_by_field3" filter="field_list" null="1" default=""/>
                        <element name="list_order_by_direction3" filter="char" null="1" default="DESC"
                                 values="ASC,DESC"/>

                        <element name="feed_theme_id" filter="integer" null="1" default="" values=""/>
                        <element name="feed_page_view_id" filter="integer" null="1" default="" values=""/>
                        <element name="feed_limit" filter="integer" null="1" default="5" minimum="0" maximum="20"/>
                        <element name="feed_email" filter="email" null="1" default="" values=""/>

                        <element name="caching" filter="binary" null="1" default="1" values="0,1"/>
                        <element name="cache_time" filter="integer" null="1" default="15" values=""/>
                        <element name="cache_handler" filter="char" null="1" default="" values=""/>

                    </customfield>
                </customfields>
            </component>
        </table>

    </config>
</extension>
