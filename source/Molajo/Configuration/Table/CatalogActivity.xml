<?xml version="1.0" encoding="utf-8"?>
<model
        name="CatalogActivity"
        table="#__catalog_activity"
        primary_key="id"
        primary_prefix="a"
        get_special_fields="0"
        get_item_children="0"
        use_special_joins="0"
        check_view_level_access="1"
        data_source="JDatabase">
    <filters>
        <filter name="id" filter="int" null="1" default="0"/>
        <filter name="catalog_id" filter="int" null="0" default="" values=""/>
        <filter name="user_id" filter="int" null="0" default="0" values=""/>
        <filter name="rating" filter="int" null="1" default="" values=""/>
        <filter name="activity_datetime" filter="datetime" null="0" default=""/>
        <filter name="ip_address" filter="char" null="0" default=""/>
        <filter name="custom_fields" filter="" null="1" default=""/>
    </filters>
    <triggers>
        <trigger name="IPAddress"/>
        <trigger name="Language"/>
    </triggers>
    <foreignkeys>
        <foreignkey name="catalog_id" source_id="id" source_model="Catalog" required="1"/>
        <foreignkey name="user_id" source_id="id" source_model="Users" required="1"/>
    </foreignkeys>
</model>

