<?xml version="1.0" encoding="utf-8"?>
<model
        name="Content"
        table="#__content"
        primary_key="id"
        name_key="title"
        primary_prefix="a"
        get_customfields="1"
        get_item_children="0"
        use_special_joins="1"
        check_view_level_access="1"
        process_triggers="1"
        data_source="JDatabase">

    <table>
        <include name="TableContent"/>
    </table>

    <customfields>
        <customfield name="customfields">
            <include name="CustomfieldsContent"/>
        </customfield>
        <customfield name="metadata">
            <include name="Metadata"/>
        </customfield>
        <customfield name="parameters">
            <include name="ParametersCustomfieldsContent"/>
        </customfield>
    </customfields>
</model>
