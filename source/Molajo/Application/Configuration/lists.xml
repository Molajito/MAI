<?xml version="1.0" encoding="utf-8"?>
<lists>
    <list name="author" model="UsersModel" key="id" value="username" ordering="username" catalogtypes="0" viewaccess="0"/>
    <list name="category" model="ContentModel" key="id" value="title" ordering="title" catalogtypes="3000"
          viewaccess="1"/>
    <list name="tag" model="ContentModel" key="id" value="title" ordering="title" catalogtypes="3500" viewaccess="1"/>
    <list name="group" model="ContentModel" key="id" value="title" ordering="title" catalogtypes="100,110,120"
          viewaccess="1"/>
    <list name="friend" model="ContentModel" key="id" value="title" ordering="title" catalogtypes="130" viewaccess="1"/>
    <list name="status" method="getStatusList"/>
    <list name="featured" method="getFeaturedList"/>
    <list name="site" model="SitesModel" key="id" value="name" ordering="name" catalogtypes="0" viewaccess="0"/>
    <list name="application" model="ApplicationsModel" key="id" value="name" ordering="name" catalogtypes="0"
          site="{{select}}"/>
    <list name="language" method="getLanguageList"/>
    <list name="component" model="ExtensionInstancesModel" key="id" value="title" ordering="title" catalogtypes="1050"
          application="{{select}}"/>
    <list name="module" model="ExtensionInstancesModel" key="id" value="title" ordering="title" catalogtypes="1350"
          application="{{select}}"/>
    <list name="theme" model="ExtensionInstancesModel" key="id" value="title" ordering="title" catalogtypes="1500"
          application="{{select}}"/>
    <list name="view_page" model="ExtensionInstancesModel" key="id" value="title" ordering="title" catalogtypes="1200"
          application="{{select}}"/>
    <list name="view_template" model="ExtensionInstancesModel" key="id" value="title" ordering="title" catalogtypes="1250"
          application="{{select}}"/>
    <list name="view_wrap" model="ExtensionInstancesModel" key="id" value="title" ordering="title" catalogtypes="1150"
          application="{{select}}"/>
</lists>
