<?xml version="1.0" encoding="utf-8"?>
<model
        name="MenuitemsItem"
        catalog_type_id="1300"
        table="#__content"
        primary_key="id"
        name_key="title"
        primary_prefix="a"
        get_customfields="1"
        get_item_children="0"
        use_special_joins="1"
        check_view_level_access="1"
        data_source="JDatabase">
    <table>
        <item>
            <fields>
                <include name="FieldsContent"/>
            </fields>
            <joins>
                <include name="JoinsContent"/>
            </joins>
            <foreignkeys>
                <include name="ForeignkeysContent"/>
            </foreignkeys>
            <triggers>
                <trigger name="Datefields"/>
            </triggers>
            <customfields>
                <customfield name="customfields">
                    <include name="CustomfieldsContent"/>
                </customfield>
                <customfield name="metadata">
                    <include name="Metadata"/>
                </customfield>
                <customfield name="parameters">
                    <field name="criteria_title" type="char" null="1" default=""/>
                    <field name="criteria_display_view_on_no_results" type="binary" null="1" default="1" values="0,1"/>
                    <field name="criteria_catalog_type_id" type="int" null="1" default="2000"
                           values="2000,2100,2200,2300"/>
                    <field name="criteria_snippet_length" type="int" null="0" default="200"/>
                    <include name="ParametersMenuitem"/>
                    <include name="ParametersGrid"/>
                    <include name="ParametersImage"/>
                    <include name="ParametersGravatar"/>
                    <include name="ParametersAsset"/>
                </customfield>
            </customfields>
        </item>
    </table>
</model>
